{
  "name": "ui-interview-codetest",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:frontend\" --names \"server,frontend\" --prefix-colors \"blue,green\"",
    "dev:server": "nodemon dev",
    "dev:frontend": "cd frontend && yarn dev",
    "build": "cd frontend && yarn build",
    "start": "cd frontend && yarn start",
    "test": "cd frontend && yarn test",
    "lint": "cd frontend && yarn lint",
    "install": "yarn install",
    "install:all": "concurrently \"yarn install\" \"cd frontend && yarn install\" --names \"root,frontend\" --prefix-colors \"blue,green\""
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.1",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.12.0",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express": "^4.18.2",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
